<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
	<classPathEntry location="WebRoot/WEB-INF/lib/mysql-connector-5.1.8.jar" />
	<context id="mysqlTables" targetRuntime="MyBatis3">
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
			<property name="suppressDate" value="true" />
		</commentGenerator>
		<!-- 数据库位置 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://127.0.0.1:3306/jf_z" userId="root"
			password="root">
		</jdbcConnection>
		
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<!-- model层包的位置 -->
		<javaModelGenerator targetPackage="com.jf.model"
			targetProject="src">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<!-- xml放置的包 -->
		<sqlMapGenerator targetPackage="com.jf.xml"
			targetProject="src">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		<!-- dao层的包 -->
		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.jf.dao" targetProject="src">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>
		<!-- 配置表和别名 -->
		<table tableName="class" domainObjectName="JFClass" />
		<!--<table tableName="groupmark" domainObjectName="JFGroupMark" />
		<table tableName="comment" domainObjectName="JFComment" />
		<table tableName="speech" domainObjectName="JFSpeech" />-->
		<!--<table tableName="groupinfo" domainObjectName="JFGroupInfo" />-->
		<!--  <table tableName="student" domainObjectName="JFStudent" />-->
		<!--<table tableName="percent" domainObjectName="JFPercent" />
		<table tableName="user" domainObjectName="JFUser" />-->
	</context>
</generatorConfiguration>